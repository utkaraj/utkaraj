Question 3:
#! /bin/bash

echo "Armstrong numbers between 1 to 999 are : "

for i in {1..999}
do
	temp_number=$i
	while [ $temp_number -ge 1 ]
	do
		temp_var=$((temp_number%10))
		power=$((temp_var*temp_var*temp_var))
		((sum=sum+power))
		temp_number=$((temp_number/10))
	done
	if [ $sum -eq $i ]
	then
		echo "$i is an armstrong number."
	fi
	sum=0
done
