Question 5:
#! /bin/bash

echo "Enter five numbers : "
read -a num

max=${num[0]}
min=${num[4]}
count=0

for i in "${num[@]}"
do		
	if [ $i -gt $max ]
	then
		max=$i
	fi
done
echo "Largest number is : $max"

for i in "${num[@]}"
do		
	if [ $i -lt $min ]
	then
		min=$i
	fi
done
echo "Smallest number is : $min"

for i in "${num[@]}"
do		
	if [ $i -eq $((i+1))]
	then
		((count=count+1))
		equal=$((i+1))	
	fi
done
echo "Number of equal numbers is : $count and the value is : $equal"		
